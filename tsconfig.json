{
	"compilerOptions": {
		"target": "ES2017",
		"module": "ES2020",
		"moduleResolution": "node",
		"sourceMap": true,
		"watch": false,
		// scoresaber.user.js
		"removeComments": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"alwaysStrict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		//,"noUnusedParameters": true
		"strictPropertyInitialization": true,
		"strictFunctionTypes": true,
		"strict": true,
	}
}
