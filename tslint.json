{
	"extends": "tslint:recommended",
	"rules": {
		"array-type": [
			true,
			"array"
		],
		"arrow-parens": false,
		"curly": false,
		"forin": true,
		"indent": false,
		"max-line-length": false,
		"no-console": false,
		"no-unnecessary-initializer": false,
		"object-literal-shorthand": false,
		"object-literal-sort-keys": false,
		"trailing-comma": false,
		"variable-name": {
			"options": [
				"ban-keywords",
				"check-format",
				"allow-snake-case"
			]
		}
	}
}
